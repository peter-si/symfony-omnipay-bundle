{
    "name": "alpy/symfony-omnipay-bundle",
    "description": "Omnipay bundle for Symfony 4.4 / 5.x / 6.x / 7.x",
    "keywords": ["omnipay", "bundle", "symfony", "payment", "php", "gateway", "credit card"],
    "homepage": "https://github.com/peter-si/symfony-omnipay-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Simoncic",
            "email": "ps@alpy.com",
            "role": "PHP developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^7.4|^8.0|^8.1|^8.2|^8.3|^8.4",
        "league/omnipay": "^3",
        "symfony/framework-bundle": "^4.4||^5.4||^6||^7",
        "symfony/cache": "^4.4||^5.4||^6||^7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpro/grumphp": "^1.3",
        "omnipay/tests": "^4",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^1.7.10",
        "omnipay/paypal": "^3.0",
        "php-http/guzzle7-adapter": "^1.0.0",
        "http-interop/http-factory-guzzle": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Janwebdev\\OmnipayBundle\\": "",
            "Janwebdev\\OmnipayBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "scripts": {
        "test": "./vendor/bin/grumphp run"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
