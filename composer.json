{
    "name": "janwebdev/symfony-omnipay-bundle",
    "description": "Omnipay bundle for Symfony 6.x / 7.x",
    "keywords": ["omnipay", "bundle", "symfony", "payment", "php", "gateway", "credit card"],
    "homepage": "https://github.com/peter-si/symfony-omnipay-bundle.git",
    "license": "MIT",
    "authors": [
        {
            "name": "Yan Rogozinsky",
            "email": "janwebdev@gmail.com",
            "role": "PHP developer"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": "^8.1|^8.2|^8.3|^8.4",
        "league/omnipay": "^3",
        "symfony/framework-bundle": "^6||^7",
        "symfony/cache": "^6||^7"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpro/grumphp-shim": "^2.12",
        "omnipay/tests": "^4",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^2.1",
        "omnipay/paypal": "^3.0",
        "php-http/guzzle7-adapter": "^1.0.0",
        "http-interop/http-factory-guzzle": "^1.2",
        "rector/rector": "^2.0",
        "symplify/config-transformer": "^12.4"
    },
    "autoload": {
        "psr-4": {
            "Janwebdev\\OmnipayBundle\\": "",
            "Janwebdev\\OmnipayBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "scripts": {
        "test": "./vendor/bin/grumphp run"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp-shim": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
